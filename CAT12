#include <stdio.h>
int main()
{
    int sum=0,sum2=0,k=0;
    for(int i=1000;i<10000;i++)
    {
        sum+=i;
    }
    for(int x=0;x==0;)
    {
        while(sum>0)
        {
            k=sum%10;
            sum=sum/10;
            sum2+=k;
        }
        if(sum2>9)
        {
            sum=sum2;
            sum2=0;
        }
        else
        {
            break;
        }
    }
    if(sum2%2==0)
    {
        printf("Even Found");
    }
    else
    {
        printf("Odd Found");
    }
}
